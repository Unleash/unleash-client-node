{
  "name": "unleash-client",
  "version": "0.1.1",
  "description": "Unleash Client for Node",
  "main": "lib/unleash",
  "scripts": {
    "lint": "eslint .",
    "build": "tsc lib/*",
    "test": "ava test",
    "coverage": "nyc --check-coverage=true npm test",
    "test-and-coveralls": "tsc lib/**.ts --sourceMap && nyc --reporter=lcov npm test -- --check-coverage=false && cat ./coverage/lcov-report/lcov.info | coveralls && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:finn-no/unleash-client-node.git"
  },
  "keywords": [
    "unleash",
    "client",
    "feature toggle"
  ],
  "bugs": {
    "url": "https://github.com/finn-no/unleash-client-node/issues"
  },
  "homepage": "https://github.com/finn-no/unleash-client-node",
  "dependencies": {
    "debug": "^2.2.0",
    "request": "^2.74.0"
  },
  "engines": {
    "node": "6"
  },
  "files": [
    "lib",
    "examples"
  ],
  "devDependencies": {
    "@types/node": "^6.0.41",
    "@types/request": "0.0.31",
    "ava": "^0.16.0",
    "coveralls": "^2.11.2",
    "eslint": "^3.3.1",
    "eslint-config-finn": "^1.0.0-alpha.9",
    "mkdirp": "^0.5.1",
    "nock": "^1.2.1",
    "nyc": "^8.3.0",
    "typescript": "^2.0.3"
  },
  "nyc": {
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "lib/**.js"
    ],
    "all": true
  }
}
